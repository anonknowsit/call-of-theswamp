{
  "name": "call-of-the-swamp",
  "displayName": "Call of the Swamp",
  "repository": { 
  "type": "git", 
  "url": "https://github.com/anonknowsit/call-of-theswamp.git"
  },
  "icon": "call-of-the-swamp.jpeg",
  "publisher": "Anonknowsit",
  "description": "This VSCode extension theme will transform your VSCode to a swamp-like environment.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Themes"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "themes": [
      {
        "label": "Call of the Swamp",
        "uiTheme": "vs-dark",
        "path": "./themes/call-of-the-swamp-color-theme.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}